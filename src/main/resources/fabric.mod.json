{
  "schemaVersion": 1,
  "id": "pvp-trainer",
  "version": "${version}",
  "name": "PVP Trainer",
  "description": "A mod that helps you in PVP Training",
  "authors": [
    "obscure021"
  ],
  "contact": {
    "homepage": "https://github.com/obscure021",
    "email": "hollowed.main@gmail.com",
    "sources": "https://github.com/obscure021/pvp-trainer-hud"
  },
  "license": "All-Rights-Reserved",
  "icon": "assets/pvp-trainer/icon.png",
  "environment": "client",
  "entrypoints": {
    "fabric-datagen": [
      "com.obscure.pvpTrainer.client.PvpTrainerDataGenerator"
    ],
    "client": [
      "com.obscure.pvpTrainer.client.PvpTrainerClient"
    ],
    "main": [
      "com.obscure.pvpTrainer.PvpTrainer"
    ],
    "modmenu": [
      "com.obscure.pvpTrainer.client.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "pvp-trainer.mixins.json",
    {
      "config": "pvp-trainer.client.mixins.json",
      "environment": "client"
    }
  ],
  "depends": {
    "fabricloader": ">=${loader_version}",
    "fabric": "*",
    "minecraft": "${minecraft_version}",
    "cloth-config": "*"
  },
  "suggests": {
    "modmenu": "*"
  }
}
